<block>
    <view class="history-index">
        <view class="item" wx:key="{{item.$$original.useEffic}}" bindtap="handleToDetail"
        wx:for="{{loopArray0}}" wx:for-item="item" data-e-handleToDetail-so="this"
        data-e-handleToDetail-a-a="{{item.$$original}}">
            <view class="left">
                <view class="cirle"></view>
                <view class="{{item.$loopState__temp2}}"></view>
            </view>
            <view class="right">
                <view class="right-top">
                    <view class="text">{{item.$loopState__temp4}}</view>
                    <image src="{{Arrow}}" class="navigator-arrow"
                    />
                </view>
                <view class="right-bottom">
                    <view class="time">
                        <view class="item-title">点火</view>
                        <view class="item-value">{{item.$loopState__temp6}}</view>
                    </view>
                    <view class="distance">
                        <view class="item-title">行驶</view>
                        <view class="item-value">{{item.$loopState__temp8}} 公里</view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</block>